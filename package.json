{
  "name": "gp-block-spacing",
  "version": "1.0.0",
  "description": "Add custom spacing components to gutenberg blocks.",
  "author": "German Pichardo",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "wordpress",
    "gutenberg",
    "blocks"
  ],
  "browserslist": [
    "last 40 versions"
  ],
  "homepage": "https://github.com/german-pichardo/gp-block-spacing",
  "repository": {
    "type": "git",
    "url": "https://github.com/german-pichardo/gp-block-spacing"
  },
  "bugs": {
    "url": "https://github.com/german-pichardo/gp-block-spacing/issues"
  },
  "main": "index.js",
  "devDependencies": {
    "@wordpress/base-styles": "^4.0.3",
    "@wordpress/editor": "^12.0.1",
    "@wordpress/i18n": "^4.2.3",
    "@wordpress/scripts": "^19.0.0",
    "@wordpress/stylelint-config": "^19.1.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "classnames": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "npm:wp-prettier@^2.0.5",
    "stylelint-order": "5.0.0",
    "toml": "^3.0.0"
  },
  "scripts": {
    "build": "npm-run-all makepot build-assets",
    "build-assets": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format",
    "lint:all": "npm-run-all lint:style:fix format:js lint:php:fix lint:pkg-json lint:md:fix",
    "lint:style": "wp-scripts lint-style",
    "lint:style:fix": "wp-scripts lint-style --fix",
    "lint:js": "wp-scripts lint-js",
    "lint:js:fix": "wp-scripts lint-js --fix",
    "lint:php": "composer lint",
    "lint:php:fix": "composer lint:fix",
    "lint:pkg-json": "wp-scripts lint-pkg-json ./package.json --fix",
    "lint:md:fix": "npm-run-all lint:md-docs:fix lint:md-js:fix",
    "lint:md-js:fix": "wp-scripts lint-md-js ./*.md --fix",
    "lint:md-docs:fix": "wp-scripts lint-md-docs ./*.md --fix",
    "packages-update": "wp-scripts packages-update",
    "prettier:js": "prettier --write",
    "start": "wp-scripts start",
    "makepot": "wp i18n make-pot . languages/gp-block-spacing.pot --domain=gp-block-spacing --package-name='GP Block Spacing' --exclude=node_modules,vendor",
    "postinstall": "wp-scripts build && composer install"
  }
}
